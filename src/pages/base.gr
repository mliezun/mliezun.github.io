class Post {
    init(title, excerpt, author, tags, content) {
        this.title = title
        this.excerpt = excerpt
        this.author = author
        this.tags = tags
        this.image = ""
        this.content = [
            ["article", [], content],
        ]
    }

    setImage(image) {
        this.image = image
        return this
    }
}
